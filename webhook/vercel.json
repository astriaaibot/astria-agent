{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_key",
    "CALCOM_API_KEY": "@calcom_api_key",
    "CALCOM_WEBHOOK_SECRET": "@calcom_webhook_secret"
  }
}
